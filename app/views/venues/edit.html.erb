<div class="container">
  <h1> Edit details of <%=@venue.name%>  </h1>
  <%= simple_form_for(@venue) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :capacity %>
    <%= f.input :contact_number %>
    <%= f.submit%>
  <% end %>
</div>

<%= link_to "Back to all venues", venues_path, class: "btn btn-primary"%>
