<div class="container">
  <h1> Edit details of <%=@venue.name%>  </h1>
  <%= simple_form_for(@venue) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :capacity %>
    <%= f.input :contact_number %>
    <%= f.submit, class: "btn btn-gradient" %>
  <% end %>
</div>
